                <mvn.mysql.numTestsPerEvictionRun>16</mvn.mysql.numTestsPerEvictionRun>
                
                <mvn.mysql.logAbandoned>true</mvn.mysql.logAbandoned>
                <mvn.mysql.removeAbandoned>true</mvn.mysql.removeAbandoned>
